<?xml version="1.0"?>
<configuration> 

	<actuator id="BZ1">
		<name>Buzzer</name>
		<class>buzzer</class>
		<location>lab</location>
		<state id="BeepEmergency">State one </state>
		<state id="IntruderAlert">Earthquake!</state>
		<ws-path>./plugins/Buzzer.jar</ws-path>
	</actuator>
	<actuator id="CAM1">
		<name>Web cam</name>
		<class>camera</class>
		<location>lab</location>		
		<ws-path>./plugins/WebCam.jar</ws-path>
	</actuator>
	<sensor id="ETH1">
		<name>Earthquake</name>
		<class>seismic</class>
		<location>lab</location>
		<state id="Earthquake high">Major quake</state>
		<state id="Earthquake Low">Mild tremor</state>
		<ws-path>./plugins/seismic.jar</ws-path>
	</sensor>
	<actuator id="ETHC1">
		<name>Earthquake Conf</name>
		<class>seismic conf</class>
		<location>lab</location>		
		<ws-path>./plugins/seismic.jar</ws-path>
	</actuator>
	<sensor id="PHO1">
		<name>Light Sensor</name>
		<class>photonic</class>
		<location>lab</location>
		<state id="Light on">Lights are on</state>
		<state id="Light off">Lights are off</state>
		<ws-path>./plugins/light.jar</ws-path>
	</sensor>
	<actuator id="PHC1">
		<name>Light Sensor conf</name>
		<class>photonic conf</class>
		<location>lab</location>		
		<ws-path>./plugins/light.jar</ws-path>
	</actuator>
	<sensor id="TMP1">
		<name>Temperature Sensor</name>
		<class>thermal</class>
		<location>lab</location>
		<state id="Temperature warm">Its warm</state>
		<state id="Temperature hot">Its hot</state>
		<state id="Temperature cold">Its cold</state>
		<ws-path>./plugins/temp.jar</ws-path>
	</sensor>
	<actuator id="TMPC1">
		<name>Temperature Sensor</name>
		<class>thermal conf</class>
		<location>lab</location>		
		<ws-path>./plugins/temp.jar</ws-path>
	</actuator>
	<sensor id="MK1">
		<name>Makey Makey</name>
		<class>tactile</class>
		<location>lab</location>
		<state id="Pressed">Intrusion detected</state>
		<state id="Released">Normal</state>
		<ws-path>./plugins/makey.jar</ws-path>
	</sensor>
	<sensor id="RF1">
		<name>RFID</name>
		<class>RF</class>
		<location>lab</location>
		<state id="Student In">Student In</state>
		<state id="Student Out">Student Out</state>
		<ws-path>./plugins/rfid.jar</ws-path>
	</sensor>		
</configuration> 
